    <result>FAILURE</result>
    <script>pipeline {
    agent any

    environment {
        COVERAGE_SCRIPT = &quot;python3 tests/check_coverage.py&quot;
        TEST_COMMAND = &quot;pytest --cov=gui_ike_debugger tests/&quot;
        MIN_COVERAGE = &quot;67.28&quot;
        WORKSPACE_PATH = &quot;gui_ike_debugger&quot;
        TEST_PATH = &quot;gui_ike_debugger/tests&quot;
        TEST_RESULT_FILE = &quot;pytest_results.xml&quot;  // Path to store pytest result as XML
    }

    stages {
        stage(&apos;Clean Workspace&apos;) {
            steps {
                cleanWs()
            }
        }

        stage(&apos;Clone Project&apos;) {
            steps {
                // Replace this with the actual source or git repo as needed
                sh &quot;cp -r /home/jaskirat/gui_project/gui_ike_debugger .&quot;
            }
        }

        stage(&apos;Run Tests with Coverage&apos;) {
            steps {
                dir(&quot;${WORKSPACE_PATH}&quot;) {
                    // Run pytest with coverage first and generate the result file
                    sh &quot;${TEST_COMMAND} --junitxml=${TEST_RESULT_FILE}&quot;

                    // Run pytest with verbose output and capture the result
                    script {
                        def pytestResult = sh(script: &quot;pytest -v&quot;, returnStatus: true, returnStdout: true).trim()
                        echo &quot;Pytest -v Output:\n${pytestResult}&quot;

                        // Check if pytest was successful by inspecting the result
                        if (pytestResult.contains(&quot;failed&quot;)) {
                            echo &quot;❌ Tests failed!&quot;
                            currentBuild.result = &apos;UNSTABLE&apos;
                            mail to: &apos;jassep1974@gmail.com&apos;,
                                 subject: &quot;Test Failure Notification&quot;,
                                 body: &quot;One or more tests have failed in the pytest run:\n\n${pytestResult}&quot;
                        } else {
                            echo &quot;✅ All tests passed!&quot;
                        }
                    }
                }
            }
        }

        stage(&apos;Check Coverage&apos;) {
            steps {
                dir(&quot;${WORKSPACE_PATH}&quot;) {
                    script {
                        def output = sh(script: COVERAGE_SCRIPT, returnStdout: true).trim()
                        echo &quot;Coverage Output:\n${output}&quot;

                        def matcher = output =~ /Coverage:\s+([0-9.]+)/
                        if (matcher) {
                            def coverageString = matcher[0][1]
                            def coverage = coverageString.toDouble()
                            echo &quot;Extracted coverage: ${coverage}&quot;

                            if (coverage &gt;= MIN_COVERAGE.toDouble()) {
                                echo &quot;✅ Coverage above threshold&quot;
                            } else {
                                echo &quot;❌ Coverage below threshold&quot;
                                currentBuild.result = &apos;UNSTABLE&apos;
                                mail to: &apos;jassep1974@gmail.com&apos;,
                                     subject: &quot;Code Coverage Alert: ${coverage}%&quot;,
                                     body: &quot;Coverage has dropped below ${MIN_COVERAGE}%.\n\n${output}&quot;
                            }
                        } else {
                            error &quot;Failed to parse coverage output&quot;
                        }
                    }
                }
            }
        }

        stage(&apos;Publish Test Results&apos;) {
            steps {
                echo &apos;Publishing test results...&apos;
                // Publish the test results to Jenkins for visualization
                junit &apos;**/pytest_results.xml&apos;  // Adjust path if necessary
            }
        }
    }

    triggers {
        cron(&apos;H 1 * * *&apos;) // Runs daily at a random minute in the 1AM hour
    }

    post {
        always {
            echo &apos;Cleaning up after the build...&apos;
        }

        success {
            echo &apos;Tests passed successfully and coverage is within threshold!&apos;
        }

        failure {
            echo &apos;Tests failed or coverage is below threshold. Please check the logs for details.&apos;
        }
    }
}
</script>
